<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wanghao.jnshu.dao.TokensDao">
    <!--目的:为dao接口方法提供sql语句配置
    即针对dao接口中的方法编写我们的sql语句-->
    <select id="select" resultType="Tokens">
        SELECT *
        FROM tokens WHERE token=#{token}
    </select>
    <insert id="insert" >
        INSERT INTO tokens(username_id,token,create_date) VALUES (#{usernameId},#{token},#{createDate});
    </insert>

</mapper>